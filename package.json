    "url": "https://github.com/pleten/interview_test_task/issues"
  },
  "homepage": "https://github.com/pleten/interview_test_task#readme",
  "dependencies": {
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.0.10",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "protractor-beautiful-reporter": "^1.2.8",
    "rimraf": "^2.6.3",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  }
