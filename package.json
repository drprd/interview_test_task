{
  "name": "ssls-com-frontend-tests",
  "version": "1.0.0",
  "description": "Automated tests for SSLs.com frontend functionality",
  "scripts": {
    "clean": "rimraf dist/*",
    "install": "webdriver-manager update",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "protractor": "protractor dist/conf.js",
    "start": "npm install && npm-run-all clean lint tsc protractor",
    "start:parallel": "set PARALLEL=true && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pleten/interview_test_task.git"
  },
